//!compile. execute with input "". expect output equals "12". endl.

(class A()
	obj x : Int32 := 0
	obj y : Int32 := 5
	obj z : Int32 := 8
	
	public proc output(in outArg : Int32)
	
	(thread
		x := y + 22
		x := x - z
		x := x * 2
		x := x div 3
		output(x)
	thread)
	
	(thread
		(accept
			output(in outArg : Int32)
		accept)
	thread)
class A)

obj a : A := new A()