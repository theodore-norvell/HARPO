//!compile. execute with input "". expect output equals "5". endl.

// This case tests:
//		- 2 classes, single variable
//		- Variable gets output from 2nd class 

(class A()
	public proc output(in outArg : Int32)
	(thread
		(accept 
			output(in outArg : Int32)
		accept)
	thread)
class A)

(class B()
	obj b1 : Int32 := 5
	obj a : A := new A()
	(thread
		a.output(b1)
	thread)
class B)


obj b : B := new B()