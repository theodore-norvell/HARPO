//!compile. execute with input "". expect output matches "2.5[0]*". endl.

(class A()
	obj x : Int16 := 0
	obj y : Real32 := 0
	
	public proc output(in outArg : Real32)
	
	(thread
		x := 26 mod 7 as Int16
		y := x / 2.0 as Real32
		output(y)
	thread)
	
	(thread
		(accept
			output(in outArg : Real32)
		accept)
	thread)
class A)

obj a : A := new A()